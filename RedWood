-- Gui to Lua
-- Version: 3.2

-- Instances:

local RedWood = Instance.new("ScreenGui")
local RedWoodGUI = Instance.new("Frame")
local Close = Instance.new("TextButton")
local Yeet = Instance.new("TextButton")
local TextLabel = Instance.new("TextLabel")
local reanim = Instance.new("TextButton")

--Properties:

RedWood.Name = "RedWood"
RedWood.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
RedWood.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

RedWoodGUI.Name = "RedWoodGUI"
RedWoodGUI.Parent = RedWood
RedWoodGUI.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
RedWoodGUI.BorderSizePixel = 0
RedWoodGUI.Position = UDim2.new(0.00729927002, 0, 0.590457201, 0)
RedWoodGUI.Size = UDim2.new(0, 228, 0, 200)
RedWoodGUI.Visible = false

Close.Name = "Close"
Close.Parent = RedWoodGUI
Close.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Close.BorderSizePixel = 0
Close.Position = UDim2.new(0.75111109, 0, 0, 0)
Close.Size = UDim2.new(0, 56, 0, 40)
Close.Font = Enum.Font.SourceSans
Close.Text = "Close"
Close.TextColor3 = Color3.fromRGB(255, 255, 255)
Close.TextSize = 14.000

Yeet.Name = "Yeet"
Yeet.Parent = RedWoodGUI
Yeet.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Yeet.BorderSizePixel = 0
Yeet.Position = UDim2.new(0, 0, 0.293532342, 0)
Yeet.Size = UDim2.new(0, 125, 0, 50)
Yeet.Font = Enum.Font.SourceSans
