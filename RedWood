-- Scripts:

local function UISCDTD_fake_script() -- RedWoodGUI.Drag 
	local script = Instance.new('Script', RedWoodGUI)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(UISCDTD_fake_script)()
local function RRKL_fake_script() -- Close.Script 
	local script = Instance.new('Script', Close)

	script.Parent.Parent:Destroy()
end
coroutine.wrap(RRKL_fake_script)()
local function NFCYS_fake_script() -- Yeet.Script 
	local script = Instance.new('Script', Yeet)

	function executed()
		loadstring(game:GetObjects('rbxassetid://5671578245')[1].Source)()
	end
	
	
	script.Parent.MouseButton1Click:Connect(executed)
	
end
coroutine.wrap(NFCYS_fake_script)()
local function GGNUCCA_fake_script() -- reanim.Script 
	local script = Instance.new('Script', reanim)

	function reanim()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Tescalus/Pendulum-Hubs-Source/main/Pendulum%20Hub%20V4.lua"))()
		loadstring(game:HttpGet(('https://pastebin.com/raw/Cu7bKQWN'),true))()
	end
	
	script.Parent.MouseButton1Click:Connect(reanim)
end
coroutine.wrap(GGNUCCA_fake_script)()
